<UserControl x:Class="MusicClubManager.Cms.Wpf.UserControls.PerformanceUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusicClubManager.Cms.Wpf.UserControls"
             xmlns:vm="clr-namespace:MusicClubManager.Cms.Wpf.ViewModels"            
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=vm:PerformanceViewModel}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock Text="{Binding ArtistName}"/>
    </Grid>
</UserControl>
